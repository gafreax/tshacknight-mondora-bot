    },
    "dependencies": {
        "axios": "^0.15.3",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-plugin-rewire": "^1.1.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "botbuilder": "^3.14.0",
        "botbuilder-azure": "^3.0.4",
        "bunyan": "^1.8.5",
        "bunyan-request": "^1.2.0",
        "cors": "^2.8.4",
